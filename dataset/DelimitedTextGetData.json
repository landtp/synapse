{
	"name": "DelimitedTextGetData",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lanworkspace200-WorkspaceDefaultStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"outputfileName": {
				"type": "string",
				"defaultValue": "csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().outputfileName,'.csv')",
					"type": "Expression"
				},
				"folderPath": "CSV",
				"fileSystem": "lanfilesys200"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}